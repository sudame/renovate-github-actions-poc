{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": "\\.github/workflows/.+\\.ya?ml$",
      "matchStrings": ["image: (?<depName>.*?):(?<currentValue>.*?)\\n"],
      "datasourceTemplate": "docker"
    }
  ]
}
