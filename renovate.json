{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": "\\.github/workflows/.+\\.ya?ml$",
      "matchStrings": [
        "image: cypress\\/browsers:(?<currentValue>.*chrome-(?<extractVersion>.*?)-ff.*?)\\n"
      ],
      "datasourceTemplate": "docker",
      "depNameTemplate": "cypress/browsers",
      "versioningTemplate": ""
    }
  ]
}
